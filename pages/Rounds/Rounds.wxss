
.profile-wrapper, .account-wrapper {
    background-color: rgb(255, 255, 255);
    margin-bottom: 20rpx;
}

.profile, .account {
    display: flex;
    padding: 30rpx;
    align-items: center;
    flex-direction: row;
    border-bottom: 1rpx solid #f0f0f0;
}

.profile-item, .account-item {
    flex: 1;
    font-size: 40rpx;

}

.profile-arrow, .account-arrow {
    width: 32rpx;
    height: 32rpx;
}
.title-arrow{
    width: 80rpx;
    height: 80rpx;
}
.green{
  background: rgb(6, 81, 241) !important;
}
