<!--pages/carnum/carnum.wxml-->
<camera
  device-position="back"
  flash="off"
  binderror="error"
  style="width: 100%; height: 300px;"
></camera>
<button type="primary" bindtap="takePhoto">人像抓拍</button>
<button type="primary" bindtap="choosePhoto">选择照片</button>
<view>预览</view>
<image mode="widthFix" src="{{imagePath}}"></image>
<button type="primary" bindtap="CarOcr" disabled='{{disabled}}' class="tiqu" >提取</button>


<button type="primary" bindtap="save"disabled='{{sd}}' class="baocun">保存</button>
